<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
</head>
<body>
    <div class="page">
        <div id="header">
            <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>
            <div id="title">
                <p><center><img src=@Url.Content("~/Images/tranSapo.jpg") width="600" height="225"></center></p>
            </div>
            <div id="menucontainer">
                <ul id="menu">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("Testing", "Index", "Testing")</li>
                </ul>
            </div>      
        </div>
        <div id="main">
            @RenderBody()
        </div>
        <div id="footer">
            
        </div>
    </div>
</body>
</html>
